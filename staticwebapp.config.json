{
  "routes": [
    {
      "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
          "/images/*.{png,jpg,gif}",
          "/css/*"
        ]
      }
    },
    {
      "route": "/react/*",
      "rewrite": "/react/index.html",
      "statusCode": 200
    },
    {
      "route": "/webcomponents/*",
      "rewrite": "/webcomponents/index.html",
      "statusCode": 200
    },
    {
      "route": "/vue/*",
      "rewrite": "/vue/index.html",
      "statusCode": 200
    },
    {
      "route": "/vue/about",
      "rewrite": "/vue/index.html",
      "statusCode": 200,
      "headers": {
        "test": "testing"
      }
    },
    {
      "route": "/reactnative/*",
      "rewrite": "/reactnative/index.html",
      "statusCode": 200
    }
  ]
}