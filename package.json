{
  "name": "@study/source",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "build": "nx affected --target build",
    "clean:deps": "rm -rf ./node_modules",
    "clean:full": "pnpm run clean:git && pnpm run clean:deps && pnpm install && pnpm run reset:nx",
    "clean:git": "git clean -fdx -e \"!.env\" -e \"node_modules\" && git reset --hard HEAD",
    "lint": "nx format:write && nx affected --target lint --fix",
    "nvm:setup": "nvm use && nvm install && nvm alias default 22.16.0",
    "prepare": "husky install",
    "reset:nx": "npx nx reset",
    "start": "nx run-many --target dev",
    "test": "nx format:check && nx affected -t typecheck,lint,test,build",
    "testwithoutnxcloud": "nx format:check && nx affected -t typecheck,lint,test,build --skip-nx-cloud"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,json,md,yml}": [
      "nx format:write --uncommitted"
    ],
    "*.{js,jsx,ts,tsx,vue}": [
      "nx affected --target=lint --uncommitted --fix"
    ]
  },
  "dependencies": {
